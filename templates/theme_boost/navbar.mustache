{{!
    Page navbar.
}}
<nav class="fixed-top navbar px-3 px-xl-0 {{# additionalinfo.isloggedin }} navbar-light {{/additionalinfo.isloggedin}}  {{^ additionalinfo.isloggedin }} navbar-dark {{/additionalinfo.isloggedin}} navbar-expand moodle-has-zindex" aria-label="{{#str}}sitemenubar, admin{{/str}}">

    <a href="{{{ config.wwwroot }}}" class="navbar-brand nav-link px-1  has-logo d-flex align-items-center">
        <img src="{{output.get_compact_logo_url}}" alt="{{sitename}}" class="img-fluid navbar__logo">
    </a>
    {{#primarymoremenu}}
        <div class="primary-navigation">
            {{> core/moremenu}}
        </div>
    {{/primarymoremenu}}

    <ul class="navbar-nav d-none d-md-flex ml-auto align-self-stretch">
        <li class="d-none d-lg-flex"  id="nav-search-box">
            {{{ output.search_box }}}
        </li>
        {{{ output.lang_menu }}}
        {{{ output.page_heading_menu }}}
    </ul>
    <ul class="nav navbar-nav align-self-stretch">
        <!-- navbar_plugin_output -->
        <li class="nav-item d-none d-lg-flex" id="nav-user-icons">
            {{{ output.navbar_plugin_output }}}
        </li>
        <!-- user_menu -->
        <li class="nav-item d-flex align-items-center">
            {{{ output.user_menu }}}
        </li>
    </ul>

     {{{ output.edit_switch }}}
</nav>
<div class="nav-border-bottom"></div>

